{
  "dashboard": {
    "id": null,
    "title": "Document Learning App - Platform Overview",
    "tags": ["document-learning", "overview"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Active Users by Platform",
        "type": "stat",
        "targets": [
          {
            "expr": "sum by (platform) (active_sessions_total)",
            "legendFormat": "{{platform}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list",
              "orientation": "horizontal"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Events per Platform",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(analytics_events_total[5m])",
            "legendFormat": "{{platform}} - {{event_type}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Performance Alerts",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(performance_alerts_total[5m])",
            "legendFormat": "{{platform}} - {{severity}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Feedback Submissions",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(feedback_submissions_total[5m])",
            "legendFormat": "{{platform}} - {{feedback_type}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "A/B Test Assignments",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(ab_test_assignments_total[5m])",
            "legendFormat": "{{test_name}} - {{variant_name}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 16
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s"
  }
}